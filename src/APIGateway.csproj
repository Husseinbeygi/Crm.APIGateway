    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>4e49957a-a39c-4fb9-ab81-e79c25ba6a10</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
